// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum State {
  ACTIVE
  INACTIVE
  DELETED
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  lastName String
  email    String @unique
  password String
  state    State  @default(ACTIVE)
}
